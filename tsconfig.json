{
    "compilerOptions": {
        "target": "es5",
        "module": "esnext",
        "lib": [
            "es2019",
            "dom",
            "dom.iterable"
        ],
        "jsx": "preserve",
        "strict": true,
        "noImplicitAny": false,
        "esModuleInterop": true,
        "allowJs": true,
        "skipLibCheck": true,
        "allowSyntheticDefaultImports": true,
        "forceConsistentCasingInFileNames": true,
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "noEmit": true
    }
}
